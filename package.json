{
  "name": "rating-system",
  "version": "1.0.19",
  "description": "Rating System. Tracks contract rates for various in-land cargo transportation. Generates accurate price quotes for point to point lanes",
  "main": "server.js",
  "bin": "./bin/www/server.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./out/,.ejs,**/*.test.js,**/__mocks__,./views,./app --copy-files --no-comments",
    "start": "npm run build && cd ./dist && node ./bin/www/server.js",
    "clean": "rimraf dist/",
    "clean-doc": "rimraf out/",
    "doc": "npm run clean-doc && jsdoc -c ./jsdoc.conf.json",
    "git-feature": "npm run clean && npm run build && git add . && git commit -m 'new feature $MESSAGE added' && git push origin refactor && npm version minor",
    "git-bug": "npm run clean && npm run build && git add . && git commit -m 'bug(s) $MESSAGE patched' && git push origin refactor && npm version patch",
    "rebuild-dependencies": "rm -rf npm_modules/ && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TITANJC/Rating-System"
  },
  "author": "R&B Programming Team",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.54",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.1",
    "config": "^1.25.1",
    "connect-loki": "^1.1.0",
    "cookie-parser": "~1.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "express-load-routes": "^1.0.1",
    "express-session": "^1.0.4",
    "idb-connector": "^1.0.13",
    "lokijs": "^1.5.3",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.9",
    "morgan": "~1.0.0",
    "multer": "latest",
    "node4mailer": "^4.0.3",
    "passport": "^0.4.0",
    "passport-cookie": "^1.0.6",
    "passport-local": "^1.0.0",
    "soap": "^0.19.0",
    "socket.io": "^2.0.4",
    "underscore": "^1.8.3",
    "uuid": "^3.2.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/register": "^7.0.0-rc.1",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2"
  }
}
